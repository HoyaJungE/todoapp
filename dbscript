
  CREATE TABLE "SHOPPING"."GOODS"
    (	"GOODS_NO" NUMBER NOT NULL ENABLE,
 	"GOODS_CATEGORY" VARCHAR2(100 BYTE) NOT NULL ENABLE,
 	"GOODS_NAME" VARCHAR2(100 BYTE) NOT NULL ENABLE,
 	"GOODS_CONTENT" VARCHAR2(4000 BYTE) NOT NULL ENABLE,
 	"GOODS_ORIGIN_PRICE" NUMBER,
 	"GOODS_SELL_PRICE" NUMBER,
 	"GOODS_SALE_PRICE" NUMBER,
 	"GOODS_DATE" DATE NOT NULL ENABLE,
 	"GOODS_KEYWORD" VARCHAR2(100 BYTE),
 	"GOODS_READCNT" NUMBER DEFAULT 0,
 	"GOODS_PICK" VARCHAR2(100 BYTE) DEFAULT 0,
 	"GOODS_THUMBNAIL" VARCHAR2(1000 BYTE),
 	"GUBUN" CHAR(1 BYTE) DEFAULT 0,
 	"FILE_NO" NUMBER,
 	 CONSTRAINT "GOODS_PK" PRIMARY KEY ("GOODS_NO")
   USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS
   STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
   PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
   TABLESPACE "SYSTEM"  ENABLE
    ) SEGMENT CREATION IMMEDIATE
   PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
   STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
   PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
   TABLESPACE "SYSTEM" ;

    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_NO" IS '상품번호';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_CATEGORY" IS '상품카테고리';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_NAME" IS '상품명';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_CONTENT" IS '내용';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_ORIGIN_PRICE" IS '상품원가';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_SELL_PRICE" IS '상품판매가';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_SALE_PRICE" IS '상품세일가';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_DATE" IS '등록일시';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_KEYWORD" IS '키워드';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_READCNT" IS '조회수';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_PICK" IS '좋아요수';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GOODS_THUMBNAIL" IS '썸네일이미지';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."GUBUN" IS '구분';
    COMMENT ON COLUMN "SHOPPING"."GOODS"."FILE_NO" IS '파일번호';

CREATE TABLE "SHOPPING"."MEMBER"
   (	"MEMBER_NO" NUMBER NOT NULL ENABLE,
	"MEMBER_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE,
	"MEMBER_PASSWD" VARCHAR2(20 BYTE) NOT NULL ENABLE,
	"MEMBER_NAME" VARCHAR2(20 BYTE) NOT NULL ENABLE,
	"MEMBER_BIRTH" DATE NOT NULL ENABLE,
	"MEMBER_EMAIL" VARCHAR2(30 BYTE) NOT NULL ENABLE,
	"MEMBER_PHONE" VARCHAR2(13 BYTE) NOT NULL ENABLE,
	"MEMBER_ZIPCODE" VARCHAR2(10 BYTE) NOT NULL ENABLE,
	"MEMBER_ADDR1" VARCHAR2(100 BYTE) NOT NULL ENABLE,
	"MEMBER_ADDR2" VARCHAR2(100 BYTE) NOT NULL ENABLE,
	"MEMBER_DATE" DATE NOT NULL ENABLE,
	"MEMBER_GRADE" VARCHAR2(20 BYTE) NOT NULL ENABLE,
	"MEMBER_TOTAL" NUMBER,
	"MEMBER_LOG" DATE NOT NULL ENABLE,
	"MEMBER_DELETE" CHAR(1 BYTE) DEFAULT '0',
	"EMAIL_AGREE" VARCHAR2(20 BYTE),
	"SMS_AGREE" VARCHAR2(20 BYTE),
	 PRIMARY KEY ("MEMBER_NO")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM"  ENABLE
   ) SEGMENT CREATION IMMEDIATE
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM" ;

   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_NO" IS '회원번호';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_ID" IS '회원ID';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_PASSWD" IS '비밀번호';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_NAME" IS '이름';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_BIRTH" IS '생년월일';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_EMAIL" IS '이메일';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_PHONE" IS '핸드폰';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_ZIPCODE" IS '우편번호';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_ADDR1" IS '주소';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_ADDR2" IS '상세주소';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_DATE" IS '가입날짜';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_GRADE" IS '등급';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_TOTAL" IS '총결제금액';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_LOG" IS '마지막접속일';
   COMMENT ON COLUMN "SHOPPING"."MEMBER"."MEMBER_DELETE" IS '삭제여부';


  CREATE TABLE "SHOPPING"."MENU"
   (	"MENU_NO" NUMBER NOT NULL ENABLE,
	"UPPR_MENU_NO" NUMBER,
	"MENU_URL" VARCHAR2(200 BYTE),
	"MENU_NAME" VARCHAR2(200 BYTE),
	"CREATE_DT" DATE,
	 CONSTRAINT "MENU_PK" PRIMARY KEY ("MENU_NO")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM"  ENABLE
   ) SEGMENT CREATION IMMEDIATE
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM" ;

   COMMENT ON COLUMN "SHOPPING"."MENU"."MENU_NO" IS '메뉴번호';
   COMMENT ON COLUMN "SHOPPING"."MENU"."UPPR_MENU_NO" IS '상위메뉴번호';
   COMMENT ON COLUMN "SHOPPING"."MENU"."MENU_URL" IS '메뉴URL';
   COMMENT ON COLUMN "SHOPPING"."MENU"."MENU_NAME" IS '메뉴이름';
   COMMENT ON COLUMN "SHOPPING"."MENU"."CREATE_DT" IS '생성일자';


  CREATE TABLE "SHOPPING"."FILE_DTL"
   (	"FILE_NO" NUMBER NOT NULL ENABLE,
   "FILE_SN" NUMBER DEFAULT NULL NOT NULL ENABLE,
	"FILE_PHYSC_NM" VARCHAR2(400 BYTE),
	"FILE_PATH" VARCHAR2(255 BYTE),
	"FILE_LOGIC_NM" VARCHAR2(255 BYTE),
	"FILE_EXTSN_NM" VARCHAR2(100 BYTE),
	"FILE_BYTE_SIZE" VARCHAR2(10 BYTE),
	"FILE_ORD" NUMBER,
	 CONSTRAINT "SYS_C008447" PRIMARY KEY ("FILE_NO", "FILE_SN")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM"  ENABLE
   ) SEGMENT CREATION IMMEDIATE
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM" ;

   COMMENT ON COLUMN "SHOPPING"."FILE_DTL"."FILE_NO" IS '첨부파일번호';
   COMMENT ON COLUMN "SHOPPING"."FILE_DTL"."FILE_PHYSC_NM" IS '첨부파일물리명';
   COMMENT ON COLUMN "SHOPPING"."FILE_DTL"."FILE_PATH" IS '첨부파일경로';
   COMMENT ON COLUMN "SHOPPING"."FILE_DTL"."FILE_LOGIC_NM" IS '첨부파일논리명';
   COMMENT ON COLUMN "SHOPPING"."FILE_DTL"."FILE_EXTSN_NM" IS '첨부파일확장자명';
   COMMENT ON COLUMN "SHOPPING"."FILE_DTL"."FILE_BYTE_SIZE" IS '첨부파일바이트사이즈';
   COMMENT ON COLUMN "SHOPPING"."FILE_DTL"."FILE_ORD" IS '첨부파일정렬순서';
   COMMENT ON COLUMN "SHOPPING"."FILE_DTL"."FILE_SN" IS '첨부파일순서';

    CREATE TABLE "SHOPPING"."ROLE"
       (	"ROLE_NO" NUMBER NOT NULL ENABLE,
    	"UPPR_ROLE_NO" NUMBER,
    	"ROLE_NM" VARCHAR2(200 BYTE),
    	"ROLE_CN" VARCHAR2(1000 BYTE),
    	 CONSTRAINT "ROLE_PK" PRIMARY KEY ("ROLE_NO")
      USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS
      STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
      PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
      TABLESPACE "SYSTEM"  ENABLE
       ) ;

       COMMENT ON COLUMN "SHOPPING"."ROLE"."ROLE_NO" IS '롤아이디';
       COMMENT ON COLUMN "SHOPPING"."ROLE"."UPPR_ROLE_NO" IS '상위롤아이디';
       COMMENT ON COLUMN "SHOPPING"."ROLE"."ROLE_NM" IS '롤이름';
       COMMENT ON COLUMN "SHOPPING"."ROLE"."ROLE_CN" IS '설명';

       CREATE TABLE "SHOPPING"."MEMBER_ROLE"
          (	"ROLD_NO" NUMBER NOT NULL ENABLE,
       	"MEMBER_NO" NUMBER NOT NULL ENABLE,
       	"INSRT_DT" DATE,
       	 CONSTRAINT "USER_ROLE_PK" PRIMARY KEY ("ROLD_NO", "MEMBER_NO")
         USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS
         STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
         PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
         TABLESPACE "SYSTEM"  ENABLE
          ) ;
-- 시퀀스
DROP SEQUENCE MENU_NO_SEQ;

CREATE SEQUENCE MENU_NO_SEQ
START WITH 1 -- 시작 번호
INCREMENT BY 1 -- 증가 값
NOCACHE -- 캐시를 사용하지 않음
NOCYCLE; -- 순환하지 않음

DROP SEQUENCE FILE_NO_SEQ;

CREATE SEQUENCE FILE_NO_SEQ
    START WITH 1
    INCREMENT BY 1
    NOCACHE
    NOCYCLE;

DROP SEQUENCE ROLE_NO_SEQ;

CREATE SEQUENCE ROLE_NO_SEQ
    START WITH 1
    INCREMENT BY 1
    NOCACHE
    NOCYCLE;